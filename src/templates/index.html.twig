{% include 'header.html.twig' %}
<h1>Strona główna</h1>
{% for post in posts %}
<div class='title' style="display: flex; justify-content: center;">
    <h1 style="display: inline;">Tytuł: {{ post.getTitle() }}</h1>
    {{ post.getTimestamp() }}
</div>

<div class="post" style="display: flex; justify-content: center;">
    <img class="img" src="{{ post.getFilename() }}" style="width: 300px; height: 400px;"><br>
</div>

{% endfor %}
{% include 'footer.html.twig' %}
